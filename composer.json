{
  "name": "meanify-co/meanify-code-review",
  "description": "Code quality tools and sniffs used by Meanify",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Barcelos",
      "email": "daniel@meanify.co"
    }
